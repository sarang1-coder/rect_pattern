
void hollow_rectangle(int n,int m)
{
	int i,j;
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=m;j++)
		{
			if(i==1||i==n||j==1||j==m)
			{
				printf("*");
			}
			else
				printf(" ");
		}
		printf("\n");
	}
}
int main()
{
	int rows,columns;
	printf("Enter rows and columns:");
	scanf("%d%d",&rows,&columns);
	hollow_rectangle(rows,columns);
	return 0;
}
